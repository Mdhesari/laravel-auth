{
    "name": "mdhesari/laravel-auth",
    "description": "This is my package laravel-auth",
    "keywords": [
        "Mdhesari",
        "laravel",
        "laravel-auth"
    ],
    "homepage": "https://github.com/mdhesari/laravel-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamad Hesari",
            "email": "mdhesari99@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "jenssegers/agent": "^2.6",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.5",
        "mdhesari/api-response": "^0.12.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Mdhesari\\LaravelAuth\\": "src",
            "Mdhesari\\LaravelAuth\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mdhesari\\LaravelAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAuth\\LaravelAuth\\LaravelAuthServiceProvider"
            ],
            "aliases": {
                "LaravelAuth": "LaravelAuth\\LaravelAuth\\Facades\\LaravelAuth"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
